<view class="user-channels">
  <view class="user-channel" wx:for="{{userChannels}}" wx:key="_id">
    <navigator url="/pages/channel/template?templateId={{item.channel.channelTemplate._id}}">
      {{item.channel.channelTemplate.name}}</navigator>
    <navigator url="/pages/channel/channel?channelId={{item.channel._id}}">
      {{item.channel.channelTemplate.keyName}}：{{item.channel.key}}</navigator>
    <view class="attrs">
      <view class="attr" wx:for="{{item.channel.channelTemplate.attrs}}" wx:key="name" wx:for-item="attr">
        {{attr.name}}：{{item.channelData.data[attr.name]}}</view>
    </view>
    <view wx:if="{{item.newData}}" bindtap="notNew" data-item-index="{{index}}">新</view>
  </view>
</view>

<view>
  <navigator url="/pages/channel/templates">探索</navigator>
</view>