.main{ 
  padding: 30px;
  min-height: 80vh;
} 
 
.logo{ 
  text-align: center; 
  font-size: 30px; 
  font-weight: 900px; 
  color: #333; 
} 
 
.inp{ 
  border: 2px #aaa solid; 
  height: 50px; 
  border-radius: 15px; 
  margin-top: 20px; 
} 
 
.templates{ 
  flex-wrap: wrap; 
  /* padding: 15px 20px; */ 
} 
 
.template{ 
  width: 25%; 
  box-sizing: border-box; 
  margin-top: 25px; 
  border-radius: 15px; 
} 
 
.template-logo{ 
  width: 35px; 
  height: 35px; 
  border-radius: 50%; 
  background: gray; 
  margin:auto; 
  padding: 5px; 
  display: flex; 
} 
 
.text-logo{ 
  font-size: 25px; 
  font-weight: 700; 
  color: #fff; 
  margin: auto; 
} 
 
.template-logo>image{ 
  width: 100%; 
  height: 100%; 
} 
 
.template-name{ 
  font-size: 14px; 
  margin-top:10px; 
  text-align: center; 
  padding: 0 5px; 
}

.logo-img{
  width: 80px;
}